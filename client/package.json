{
  "name": "loft-taxi-app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "dependencies": {
    "@material-ui/core": "3.9.3",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.2",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "classnames": "2.2.6",
    "eslint": "5.12.0",
    "eslint-plugin-react": "7.11.1",
    "formik": "1.5.8",
    "i18next": "17.0.11",
    "i18next-browser-languagedetector": "^3.0.3",
    "i18next-xhr-backend": "^3.1.1",
    "ramda": "0.26.1",
    "react": "16.8.5",
    "react-dom": "16.8.5",
    "react-i18next": "10.12.1",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8",
    "redux": "4.0.1",
    "redux-form": "8.1.0",
    "redux-saga": "1.0.2",
    "reselect": "4.0.0",
    "typescript": "3.4.5",
    "yup": "0.27.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "set CI=true && npm run test:all",
    "test:coverage": "react-scripts test --coverage",
    "precommit": "lint-staged",
    "test:all": "react-scripts test",
    "test:part": "react-scripts test --u",
    "push": "cd .. && npm run push && cd client"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 opera versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^9.0.1",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "enzyme-to-json": "3.3.4",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-notice": "0.7.7",
    "eslint-plugin-prettier": "3.0.0",
    "husky": "1.2.1",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "redux-mock-store": "1.5.3",
    "redux-saga-test-plan": "^4.0.0-beta.4"
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "node_modules/.bin/eslint --max-warnings=3000",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
